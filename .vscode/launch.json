    "version": "0.2.0",
    "configurations": [
        {
            "name": "rwsheet - Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath": "${command:python.interpreterPath}",
            "program": "${workspaceRoot}/python-read-write-sheet.py",
            "cwd": "",
            "console": "integratedTerminal",
            "env": {},
            "envFile": "${workspaceRoot}/.env",
            "debugOptions": [
                "WaitOnAbnormalExit",
                "WaitOnNormalExit"
            ]
        }
    
    ]
}